-- Create buttons for each face
for faceName, faceId in pairs(faces) do
    local button = Instance.new("TextButton")
    button.Size = UDim2.new(0, 100, 0, 50)
    button.Text = faceName
    button.Parent = player.PlayerGui.ScreenGui -- Adjust as necessary

    button.MouseButton1Click:Connect(function()
        teleportPlayer(Vector3.new(0, 10, 0)) -- Change to desired position
        changeFace(faceId)
    end)
end
