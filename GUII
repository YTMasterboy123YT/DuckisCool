local player = game.Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()

-- Function to teleport the player
local function teleportPlayer(position)
    character.HumanoidRootPart.CFrame = CFrame.new(position)
end

-- Function to change the face based on button clicked
local function changeFace(faceId)
    -- Remove existing face decals
    for _, child in ipairs(character.Head:GetChildren()) do
        if child:IsA("Decal") then
            child:Destroy()
        end
    end

    -- Create and set the new face
    local face = Instance.new("Decal")
    face.Texture = "rbxassetid://" .. faceId
    face.Parent = character.Head
end

-- Example face IDs (add more as needed)
local faces = {
