local Fluent = loadstring(game:HttpGet("https://github.com/dawid-scripts/Fluent/releases/latest/download/main.lua"))()

local Window = Fluent:CreateWindow({
    Title = "DUCKS PRIVATE GUI" .. Fluent.Version,
    SubTitle = "by Masterboy123",
    TabWidth = 160,
    Size = UDim2.fromOffset(580, 460),
    Acrylic = true,
    Theme = "Dark",
    MinimizeKey = Enum.KeyCode.LeftControl
})

-- Creating a new tab
local Tab1 = Window:AddTab("Tab 1")
local Tab2 = Window:AddTab("Tab 2")

-- Adding a button in Tab 1
Tab1:AddButton("Click Me", function()
    print("Button clicked!")
end)

-- Adding a slider in Tab 1
Tab1:AddSlider("Adjust Volume", 0, 100, 50, function(value)
    print("Volume set to: " .. value)
end)

-- Adding a toggle in Tab 2
Tab2:AddToggle("Enable Feature", false, function(enabled)
    if enabled then
        print("Feature enabled!")
    else
        print("Feature disabled!")
    end
end)

-- Adding a dropdown in Tab 2
Tab2:AddDropdown("Select Option", {"Option 1", "Option 2", "Option 3"}, function(selected)
    print("You selected: " .. selected)
end)

-- Finalizing the GUI
Fluent:Open()
