local OrionLib = loadstring(game:HttpGet('https://raw.githubusercontent.com/shlexware/Orion/main/source'))()

local Window = OrionLib:MakeWindow({
    Name = "Ducks Private GUI",
    HidePremium = false,
    SaveConfig = true,
    ConfigFolder = "OrionTest",
    Icon = "rbxassetid://112270259150403",
    IntroEnabled = true,
    IntroText = "Ducks Private Script",
    IntroIcon = "rbxassetid://112270259150403"
})

-- Example button
local Button = Window:MakeButton({
    Name = "Click Me!",
    Callback = function()
        print("Button clicked!")
    end
})

-- Show the window
OrionLib:Init()
